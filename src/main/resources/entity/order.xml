<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="orderAction" class="cn.bubbletg.xxadj.action.OrderAction">
        <property name="orderService" ref="orderService"/>
    </bean>
    <bean id="orderService" class="cn.bubbletg.xxadj.service.OrderService">
        <property name="orderDao" ref="orderDaoImpl"/>
    </bean>

    <bean id="orderDaoImpl" class="cn.bubbletg.xxadj.dao.OrderDaoImpl">
        <property name="sessionFactory" ref="sessionFactory"/>
    </bean>
</beans>
